{
  "expo": {
    "name": "MBM",
    "slug": "MBM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mbm",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs to access your location to show it on the map.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyCLKuU10kq52qu78emIM4Uh6Z1hAJ1qFp4"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.daggerdiego107.MBM",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCLKuU10kq52qu78emIM4Uh6Z1hAJ1qFp4"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-notifications",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "This app needs to access your location to show it on the map."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a1cb304c-b881-4423-896b-f91d048f95a4"
      }
    }
  }
}
